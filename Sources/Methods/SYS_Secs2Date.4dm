//%attributes = {}
  // Metodo: SYS_Secs2Date
  // =================================================================
  // Usuario (OS): abachler
  // Fecha y Hora: 28/05/03, 11:33:30
  // -------------------------------------------------------------------------------
  // Descripcion: Extrae la fecha de segundos
  // 
  //
  // Parametros
  //$1=Segundos (real)
  // =================================================================



  // DECLARACIONES
  // -------------------------------------------------------------------------------
C_REAL:C285($seconds;$1)
C_DATE:C307($date;$0)

  // INICIALIZACIONES
  // -------------------------------------------------------------------------------
$seconds:=$1

  // CUERPO DEL METODO
  // -------------------------------------------------------------------------------
$dias:=Int:C8($seconds/86400)
$remaining:=Dec:C9($seconds/86400)
$time:=Round:C94(86400*$remaining;0)
$date:=!1904-01-01!+$dias
$0:=$date

  // LIBERACION DE MEMORIA
  // -------------------------------------------------------------------------------


  // FIN DEL METODO
  // -------------------------------------------------------------------------------